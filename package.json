{
  "name": "toy-store",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node ./src/server.js --mode PROD -p 8000",
    "dev": "nodemon ./src/server.js",
    "fs": "node ./src/server.js --persistence FS",
    "memory": "node ./src/server.js --persistence MEMORY",
    "listeners": "node ./src/config/listeners.js",
    "simple": "artillery quick --count 40 --num 50 'http://localhost:7001/api/simple' -o ./src/tests/performance/simple.json",
    "complex": "artillery quick --count 40 --num 50 'http://localhost:9000/api/complex' -o ./src/tests/performance/complex.json",
    "performance": "artillery run ./config.yaml --output ./src/tests/performance/crd.json",
    "report": "artillery report ./src/tests/performance/crd.json",
    "image": "docker build -t prod .",
    "container": "docker run -p 8888:8000 prod",
    "image-dev": "docker build -t dev .",
    "container-dev": "docker run -p 8889:8000 dev"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "@faker-js/faker": "^8.1.0",
    "artillery-plugin-metrics-by-endpoint": "^1.2.0",
    "bcrypt": "^5.1.1",
    "commander": "^11.0.0",
    "connect-mongo": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mongoose": "^7.2.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.5",
    "nodemon": "^2.0.22",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "session-file-store": "^1.5.0",
    "winston": "^3.10.0"
  }
}
